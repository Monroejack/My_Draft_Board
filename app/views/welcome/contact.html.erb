<h1>Contact Us!</h1>

<div class="container">
  <ul>
  <% @players.each do |player| %>
    <li>
      <%= player.last_name %>, <%= player.first_name %>
      School <%= player.school %>,
      Position: <%= player.position %>
    </li>
    <% end %>
</ul>
</div>

<div class="container">
  <h1>All Quarterbacks</h1>
  <ul>
  <% @quarterback.each do |player| %>
    <li>
      <%= player.last_name %>, <%= player.first_name %>
      School <%= player.school %>,
      Position: <%= player.position %>
    </li>
    <% end %>
</ul>
</div>
